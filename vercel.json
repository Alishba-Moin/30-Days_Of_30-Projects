{
  "version": 2,
  "projects": {
    "countdown-timer": {
      "builds": [
        {
          "src": "countdown-timer/package.json",
          "use": "@vercel/next"
        }
      ],
      "routes": [
        {
          "src": "/countdown-timer/(.*)",
          "dest": "countdown-timer/$1"
        }
      ]
    },
    "weather-widget-app": {
      "builds": [
        {
          "src": "/weather-widget-app/package.json",
          "use": "@vercel/next"
        }
      ],
      "routes": [
        {
          "src": "/weather-widget-app/(.*)",
          "dest": "weather-widget-app/$1"
        }
      ]
    }
  }
}
